<tr>
  <td><%= timelog.date %></td>
  <td><%= timelog.hours %></td>
  <td>
    <%= link_to(edit_project_time_log_path(timelog.project, timelog), remote: true, class: "btn btn-info btn-md"){icon_of('glyphicon-edit')} %>
    <%= link_to(project_time_log_path(timelog.project, timelog), method: :delete, data: { confirm: 'Are you sure?' }, class: "btn btn-danger btn-md"){icon_of('glyphicon-trash')} %>
  </td>
</tr>
